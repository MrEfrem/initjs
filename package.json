{
  "name": "@efrem/initjs",
  "version": "2.0.1",
  "main": "dist/index.js",
  "repository": "https://github.com/MrEfrem/initjs.git",
  "author": "MrEfrem <mr.efrem@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node -r @babel/register src/index.js",
    "outdated": "yarn upgrade-interactive",
    "build": "babel src -d dist",
    "lint": "eslint src/**/*.js",
    "prepublish": "yarn lint && yarn tsc && rm -rf files/cache && mkdir files/cache && cp yarn.lock files/cache && cp -R .yarn/cache files/cache/yarn && mv files/cache/yarn/.gitignore files/cache/yarn/gitignore"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@types/node": "^13.13.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.21",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "bin": "dist/index.js",
  "files": [
    "dist",
    "files"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint && yarn tsc",
      "pre-push": "yarn lint && yarn tsc"
    }
  }
}
